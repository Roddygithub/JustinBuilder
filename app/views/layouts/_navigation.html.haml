  %li{class: cp(root_path)}
    = link_to(root_path) do
      %i.icon-home
      Home
  - if user_signed_in?
    %li.nav-header
      Teams
    %li
      = link_to('5v5 Ranked', '#')
    %li
      = link_to('5v5 Alts', '#')
    %li
      = link_to('Nuke Gimmick', '#')
    %li
      = link_to('Griefing Lawls', '#')
    %li.nav-header
      Characters
    %li
      = link_to('#') do
